    </tag>

    <tag>
        <name>breadcrumb</name>
        <tag-class>de.elbe5.cms.tag.BreadcrumbTag</tag-class>
        <body-content>empty</body-content>
    </tag>

    <tag>
        <name>modal</name>
        <tag-class>de.elbe5.cms.tag.ModalTag</tag-class>
